{
	"name": "diwe",
	"version": "1.0.0",
	"main": "src/server.js",
	"type": "module",
	"license": "MIT",
	"dependencies": {
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.6",
		"@types/mocha": "^7.0.2",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"date-fns": "^2.14.0",
		"dotenv": "^8.2.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.2",
		"pg": "^8.2.1",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.25",
		"uuid-validate": "^0.0.3",
		"uuidv4": "^6.1.0",
		"yup": "^0.32.8"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/multer": "^1.4.3",
		"@typescript-eslint/parser": "^4.31.2",
		"eslint": "^7.12.1",
		"mocha": "^8.0.1",
		"pg-god": "^1.0.11",
		"prettier": "^2.0.5",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.0.0-pre.44",
		"typescript": "^3.9.3"
	},
	"scripts": {
		"build": "tsc",
		"app:up": "docker-compose up app",
		"database:migrations": "typeorm migration:run",
		"database:up": "docker-compose up -d postgresdb",
		"database:create": "pg-god db-create --databaseName=diwe --password=docker --port=5432 --host=postgresdb --userName=postgres",
		"down": "docker-compose down",
		"devantigo": "ts-node-dev -r tsconfig-paths/register src/server.ts",
		"dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
		"start": "npx concurrently \"npm run database:create\" \"npm run typeorm migration:run\" \"npm run dev\"",
		"typeorm": "ts-node ./node_modules/typeorm/cli.js"
	}
}
